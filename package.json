{
	"name": "vscode-gtt",
  "displayName": "vscode-gtt",
  "publisher": "boksasp",
	"description": "Navigate between test and test subject",
	"version": "0.0.1",
	"icon": "assets/icon.png",
	"homepage": "https://github.com/boksasp/vscode-gtt",
	"bugs": "https://github.com/boksasp/vscode-gtt/issues",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/boksasp/vscode-gtt.git"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:gtt.goToTest"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gtt.goToTest",
				"title": "Go to test/test subject",
				"category": "vscode-gtt"
			}
		]
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
